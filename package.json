{
  "name": "vanilla-es6-jspm",
  "version": "0.6.0",
  "description": "Seed project for ES6 / jspm based apps with full production workflow",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/gulp/bin/gulp.js serve",
    "test": "./node_modules/gulp/bin/gulp.js jshint:test && ./node_modules/gulp/bin/gulp.js htmlhint:test && ./node_modules/karma/bin/karma start --single-run --browsers PhantomJS --captureConsole false",
    "test-unit": "./node_modules/karma/bin/karma start",
    "postinstall": "jspm install",
    "test-build": "./bin/test-build.sh"
  },
  "pre-commit": [
    "test"
  ],
  "pre-push": [
    "test-build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/topheman/vanilla-es6-jspm.git"
  },
  "bugs": {
    "url": "https://github.com/topheman/vanilla-es6-jspm/issues"
  },
  "keywords": [
    "ES6",
    "jspm",
    "gulp"
  ],
  "author": "Christophe Rosset",
  "license": "MIT",
  "jspm": {
    "directories": {},
    "configFile": "jspm.config.js",
    "dependencies": {
      "bootstrap": "npm:bootstrap-sass@^3.3.5",
      "clean-css": "npm:clean-css@^3.3.4",
      "css": "github:systemjs/plugin-css@^0.1.13",
      "fetch-polyfill": "npm:fetch-polyfill@^0.8.1",
      "text": "github:systemjs/plugin-text@^0.0.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.6.4",
      "babel-runtime": "npm:babel-runtime@^5.6.4",
      "core-js": "npm:core-js@^0.9.17"
    }
  },
  "devDependencies": {
    "babel": "^5.8.12",
    "browser-sync": "^2.7.10",
    "chai": "^3.2.0",
    "colors": "^1.1.2",
    "connect-modrewrite": "^0.8.1",
    "del": "^1.2.0",
    "git-rev-sync": "^1.3.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-bytediff": "^0.2.1",
    "gulp-cache": "^0.2.10",
    "gulp-changed": "^1.2.1",
    "gulp-concat": "^2.5.2",
    "gulp-filter": "^2.0.2",
    "gulp-footer": "^1.0.5",
    "gulp-header": "^1.2.2",
    "gulp-htmlhint": "^0.3.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-inject": "^1.5.0",
    "gulp-jshint": "^1.11.0",
